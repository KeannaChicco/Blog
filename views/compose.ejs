<%- include("partials/header")%>

<h1>Compose</h1>



<form method="post">
  <div class="form-group">
    <label for="labelTitle">Title</label>
    <input type="text" class="form-control" name="postTitle" placeholder="">
  </div>
  <div class="form-group">
    <label for="labelPost">Post</label>
    <textarea class="form-control" name="postText" rows="4"></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="publishButton">Publish</button>
</form>


<%- include("partials/footer")%>
